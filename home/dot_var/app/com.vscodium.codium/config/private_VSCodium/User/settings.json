{
    "diffEditor.ignoreTrimWhitespace": false,
    "git.ignoreRebaseWarning": true,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
    "C_Cpp.autocompleteAddParentheses": true,
    "terminal.integrated.enableMultiLinePasteWarning": "auto",
    "explorer.confirmDragAndDrop": false,
    "workbench.startupEditor": "none",
    "diffEditor.maxComputationTime": 0,
    "git.confirmSync": false,
    "simpleBrowser.focusLockIndicator.enabled": false,
    "editor.accessibilitySupport": "off",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell",
            "args": [
                "-NoLogo"
            ]
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Developer Powershell": {
            "path": "pwsh",
            "args": [
                "-noexit",
                "-c",
                "$vsPath = & \"${env:ProgramFiles(x86)}/Microsoft Visual Studio/Installer/vswhere.exe\" -property installationpath; Import-Module \"$vsPath/Common7/Tools/Microsoft.VisualStudio.DevShell.dll\"; Enter-VsDevShell -VsInstallPath $vsPath -SkipAutomaticLocation"
            ],
            "icon": "terminal-powershell"
        }
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "css.format.newlineBetweenRules": false,
    "debug.allowBreakpointsEverywhere": true,
    "editor.rulers": [],
    "[racket]": {
        "editor.overviewRulerBorder": false
    },
    "editor.overviewRulerBorder": false,
    "C_Cpp.debugShortcut": false,
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
    "workbench.preferredDarkColorTheme": "Ayu Mirage Bordered",
    "workbench.preferredHighContrastColorTheme": "Default High Contrast",
    "debug.onTaskErrors": "abort",
    "cmake.configureOnOpen": false,
    "C_Cpp.formatting": "vcFormat",
    "C_Cpp.vcFormat.newLine.beforeElse": false,
    "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
    "security.workspace.trust.untrustedFiles": "open",
    "[python]": {
        "editor.formatOnType": true,
        "diffEditor.ignoreTrimWhitespace": true
    },
    "C_Cpp.doxygen.generatedStyle": "/**",
    "window.titleBarStyle": "custom",
    "terminal.integrated.gpuAcceleration": "on",
    "C_Cpp.vcFormat.indent.accessSpecifiers": true,
    "glassit.alpha": 250,
    "workbench.editorAssociations": {
        "*.save": "default",
        "*.pyobj": "default"
    },
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "hexeditor.columnWidth": 32,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "workbench.commandPalette.history": 0,
    "explorer.confirmDelete": false,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "cmake.showOptionsMovedNotification": false,
    "telemetry.telemetryLevel": "off",
    "githubPullRequests.createOnPublishBranch": "never",
    "remote.autoForwardPortsSource": "hybrid",
    "C_Cpp.default.cppStandard": "gnu++17",
    "workbench.editorLargeFileConfirmation": 32,
    "notebook.editorOptionsCustomizations": {
        "editor.tabSize": 4,
        "editor.indentSize": 4,
        "editor.insertSpaces": true
    },
    "git.alwaysSignOff": true,
    "debug.terminal.clearBeforeReusing": true,
    "terminal.integrated.defaultProfile.linux": "bash",
    "window.commandCenter": false,
    "[oraclesql]": {
        "editor.suggest.showSnippets": true,
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "C_Cpp.vcFormat.space.pointerReferenceAlignment": "right",
    "workbench.layoutControl.enabled": false,
    "window.menuBarVisibility": "toggle",
    "terminal.integrated.cursorStyle": "line",
    "workbench.sideBar.location": "right",
    "workbench.iconTheme": "vscode-icons",
    "workbench.productIconTheme": "Default",
    "window.confirmSaveUntitledWorkspace": false,
    "terminal.integrated.fontSize": 12,
    "powershell.powerShellAdditionalExePaths": {
        "Powershell7": "\"C:\\Program Files\\PowerShell\\7\\pwsh.exe\""
    },
    "powershell.powerShellDefaultVersion": "Powershell7",
    "terminal.integrated.fontWeight": "bold",
    "terminal.integrated.shellIntegration.enabled": false,
    "git.autofetch": true,
    "hediet.vscode-drawio.resizeImages": null,
    "git.openRepositoryInParentFolders": "never",
    "editor.maxTokenizationLineLength": 200000,
    "antlr4.generation": {
        "mode": "external",
        "outputDir": "antlr",
        "listeners": true,
        "visitors": true,
    },
    "remote.SSH.remotePlatform": {
        "poo-vmchecker": "linux"
    },
    "editor.renderWhitespace": "all",
    "java.saveActions.organizeImports": true,
    "java.sources.organizeImports.starThreshold": 10,
    "java.sources.organizeImports.staticStarThreshold": 8,
    "javascript.preferences.organizeImports": {},
    "java.cleanup.actions": [
        "renameFileToType",
        "addOverride",
        "organizeImports",
        "addFinalModifier",
        "qualifyMembers",
        "invertEquals",
        "instanceofPatternMatch",
        // "lambdaExpressionFromAnonymousClass",
        "lambdaExpression"
    ],
    "editor.minimap.showSlider": "always",
    "C_Cpp.default.cStandard": "gnu17",
    "go.toolsManagement.autoUpdate": true,
    "editor.codeActionsOnSave": {},
    "search.useIgnoreFiles": false,
    "workbench.colorTheme": "Ayu Mirage Bordered",
    "zig.zls.enabled": "on",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.symbolCacheDownload": true,
    "julia.enableTelemetry": false,
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "git.blame.editorDecoration.enabled": true,
    "git.blame.statusBarItem.enabled": true,
    "editor.foldingMaximumRegions": 20000,
    "C_Cpp.intelliSenseEngine": "disabled",
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "C_Cpp.clang_format_style": "LLVM",
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "chat.agent.enabled": true,
    "rust-analyzer.check.command": "clippy",
    "chat.editing.confirmEditRequestRemoval": false
}